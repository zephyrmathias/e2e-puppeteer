{
  "extends": ["airbnb/base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "semi": ["error", "never"],
    "import/no-named-as-default": 0,
    "prettier/prettier": "error"
  },
  "env": {
    "node": true,
    "mocha": true,
    "jest": true
  },
  "globals": {
    "page": true,
    "browser": true,
    "context": true,
    "jestPuppeteer": true
  }
}